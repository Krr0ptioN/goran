{
  "name": "drizzle-data-access",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/data-access/drizzle/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "generate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx drizzle-kit generate:pg --schema libs/data-acces/db-data-access/src/schema.ts --out libs/data-access/drizzle/src/drizzle"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/data-acces/db-data-access/**/*.ts"]
      }
    },
    "seed": {
      "executor": "nx:run-commands",
      "configurations": {
        "seed": {
          "commands": ["ts-node libs/data-acces/db-data-access/src/migrate.ts"]
        }
      }
    },
    "db-migrate": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["ts-node libs/data-acces/db-data-access/src/migrate.ts"]
      }
    }
  }
}
